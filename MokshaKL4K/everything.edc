			target: "e.text.label";
			target: "e.text.plugin";
			target: "icon_clip";
		}
		program {
			name: "unsel";
			signal: "e,state,unselected";
			source: "e";
			action: STATE_SET "default" 0.0;
			target: "bg";
			/* target: "separator";
			 * target: "over"; */
			target: "e.text.label";
			target: "e.text.plugin";
			target: "icon_clip";
		}
		//~ program {
		    //~ name: "update";
